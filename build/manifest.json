{
    "manifest_version": 3,
    "name": "scrapper",
    "version": "0.0.1",
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_title": "Scraper",
        "default_popup": "./popup.html"
        
    },
    "permissions": ["tabs", "scripting", "activeTab"],
    "icons": {
      "32": "./assets/icons/32.png",
      "48": "./assets/icons/48.png"
    },
    "host_permissions": [
        "https://www.linkedin.com/*" 
    ]
}